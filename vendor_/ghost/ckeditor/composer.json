{
    "name": "ghost/ckeditor",
    "description": "CKEditor 5 集成laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "editor", "CKEditor","CKEditor5"],
    "homepage": "https://github.com/Ghost-die/ckeditor.git",
    "license": "MIT",
    "authors": [
        {
            "name": "ghost",
            "email": "ghost@ghost-ai.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "ghost\\CKEditor\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ghost\\CKEditor\\CKEditorServiceProvider"
            ]

        }
    }
}
